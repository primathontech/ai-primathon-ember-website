  <LinkTo @route="index" class="menu-index">
    <div class="logo-holder" type="div">
      <img src="assets/images/primathon_icon_final.png" alt="Primathon" />
    </div>
    {{!-- <div
      class="logo-holder {{unless this.activePrimathon 'olympic'}}"
      type="div"
      {{timer-callback this.onLogoClick 2000}}
    >
      <img
        src="assets/images/primathon_icon_final.png"
        alt="Primathon"
        class={{if this.activePrimathon 'visible' 'hidden'}}
      />
      <img
        src="assets/images/logo-olympic.png"
        alt="Primathon"
        class={{unless this.activePrimathon 'visible' 'hidden'}}
      />
    </div> --}}
    <div>
      <h1>
        Primathon
      </h1>
      <p class="inspired">
        Above & Beyond
      </p>
    </div>
  </LinkTo>
